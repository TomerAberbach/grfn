{
  "name": "grfn",
  "version": "1.0.1",
  "author": {
    "name": "Tomer Aberbach",
    "email": "tomeraberbach@gmail.com",
    "url": "https://tomeraberba.ch"
  },
  "description": "A tiny (~400B) utility that executes a dependency graph of async functions as concurrently as possible.",
  "keywords": [
    "graph",
    "dag",
    "dependency",
    "concurrently",
    "async",
    "function",
    "solver",
    "topological"
  ],
  "homepage": "https://github.com/TomerAberbach/grfn",
  "repository": "TomerAberbach/grfn",
  "bugs": {
    "url": "https://github.com/TomerAberbach/grfn/issues"
  },
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "type": "module",
  "engines": {
    "node": ">= 18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.min.cjs",
      "import": "./dist/index.min.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.min.cjs",
  "module": "./dist/index.min.js",
  "scripts": {
    "prepare": "tomer install",
    "format": "tomer format",
    "lint": "tomer lint",
    "typecheck": "tomer typecheck",
    "test": "tomer test",
    "build": "tomer build",
    "clean": "tomer clean dist",
    "prepublishOnly": "tomer build"
  },
  "prettier": "tomer/prettier",
  "terser": {
    "mangle": {
      "properties": {
        "regex": "^_[^_]+"
      }
    }
  },
  "dependencies": {
    "type-fest": "^4.18.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "tomer": "^3.2.1",
    "typescript": "^5.4.5"
  }
}
