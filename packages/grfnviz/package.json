{
  "type": "module",
  "name": "grfnviz",
  "version": "0.0.4",
  "author": {
    "name": "Tomer Aberbach",
    "email": "tomeraberbach@gmail.com",
    "url": "https://tomeraberba.ch"
  },
  "description": "A snazzy utility for generating PNGs, SVGs, and GIFs from grfn dependency graphs.",
  "keywords": [
    "graph",
    "dag",
    "dependency",
    "concurrently",
    "async",
    "function",
    "solver",
    "topological",
    "graphviz",
    "visualization",
    "svg",
    "gif",
    "animation"
  ],
  "files": [
    "dist",
    "src/index.d.ts"
  ],
  "module": "./dist/esm/node/index.js",
  "main": "./dist/cjs/node/index.js",
  "browser": "./dist/cjs/browser/index.js",
  "exports": {
    "node": {
      "require": "./dist/cjs/node/index.js",
      "default": "./dist/esm/node/index.js"
    },
    "require": "./dist/cjs/browser/index.js",
    "default": "./dist/esm/browser/index.js"
  },
  "types": "./src/index.d.ts",
  "homepage": "https://github.com/TomerAberbach/grfn/tree/main/packages/grfnviz",
  "repository": "TomerAberbach/grfn",
  "bugs": {
    "url": "https://github.com/TomerAberbach/grfn/issues"
  },
  "license": "Apache 2.0",
  "dependencies": {
    "@hpcc-js/wasm": "1.0.1",
    "gifencoder": "2.0.1",
    "graphviz": "0.0.9",
    "grfn": "0.0.3",
    "open": "7.3.1",
    "png-js": "1.0.0"
  },
  "devDependencies": {
    "@types/node": "14.14.21"
  },
  "engines": {
    "node": ">= 10.0.0"
  }
}
